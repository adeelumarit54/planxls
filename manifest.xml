<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
<!--  Basic Info  -->
<!-- https://github.com/adeelumarit54.github.io/planxls.git -->
<Id>a063341a-107d-4df6-a679-16ca09ec6980</Id>
<Version>1.0.0.0</Version>
<ProviderName>Contoso</ProviderName>
<DefaultLocale>en-US</DefaultLocale>
<DisplayName DefaultValue="PlanXLS"/>
<Description DefaultValue="PlanXLS Add-in with Taskpane"/>
<IconUrl DefaultValue="https://github.com/adeelumarit54.github.io/planxls.git/assets/icon-32.png"/>
<HighResolutionIconUrl DefaultValue="https://github.com/adeelumarit54.github.io/planxls.git/assets/icon-64.png"/>
<SupportUrl DefaultValue="https://www.PlanXLS.com/help"/>
<!--  Host Application  -->
<Hosts>
<Host Name="Workbook"/>
</Hosts>
<!--  Default Taskpane  -->
<DefaultSettings>
<SourceLocation DefaultValue="https://github.com/adeelumarit54.github.io/planxls.git/taskpane.html"/>
</DefaultSettings>
<Permissions>ReadWriteDocument</Permissions>
<!--  Version Overrides  -->
<VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
<Hosts>
<Host xsi:type="Workbook">
<DesktopFormFactor>
<ExtensionPoint xsi:type="PrimaryCommandSurface">
<OfficeTab id="TabHome">
<Group id="TaskpaneGroup">
<Label resid="TaskpaneGroup.Label"/>
<Icon>
<bt:Image size="16" resid="Icon.16x16"/>
<bt:Image size="32" resid="Icon.32x32"/>
<bt:Image size="80" resid="Icon.80x80"/>
</Icon>
<Control xsi:type="Button" id="TaskpaneButton">
<Label resid="TaskpaneButton.Label"/>
<Supertip>
<Title resid="TaskpaneButton.Label"/>
<Description resid="TaskpaneButton.Tooltip"/>
</Supertip>
<Icon>
<bt:Image size="16" resid="Icon.16x16"/>
<bt:Image size="32" resid="Icon.32x32"/>
<bt:Image size="80" resid="Icon.80x80"/>
</Icon>
<Action xsi:type="ShowTaskpane">
<TaskpaneId>TaskpaneButton.Id</TaskpaneId>
<SourceLocation resid="Taskpane.Url"/>
</Action>
</Control>
</Group>
</OfficeTab>
</ExtensionPoint>
</DesktopFormFactor>
</Host>
</Hosts>
<!--  Resources  -->
<Resources>
<bt:Images>
<bt:Image id="Icon.16x16" DefaultValue="https://github.com/adeelumarit54.github.io/planxls.git/assets/icon-16.png"/>
<bt:Image id="Icon.32x32" DefaultValue="https://github.com/adeelumarit54.github.io/planxls.git/assets/icon-32.png"/>
<bt:Image id="Icon.80x80" DefaultValue="https://github.com/adeelumarit54.github.io/planxls.git/assets/icon-80.png"/>
</bt:Images>
<bt:Urls>
<bt:Url id="Taskpane.Url" DefaultValue="https://github.com/adeelumarit54.github.io/planxls.git/taskpane.html"/>
</bt:Urls>
<bt:ShortStrings>
<bt:String id="TaskpaneGroup.Label" DefaultValue="PlanXLS"/>
<bt:String id="TaskpaneButton.Label" DefaultValue="PlanXLS"/>
</bt:ShortStrings>
<bt:LongStrings>
<bt:String id="TaskpaneButton.Tooltip" DefaultValue="Click PlanXLS to open the taskpane"/>
</bt:LongStrings>
</Resources>
</VersionOverrides>
</OfficeApp>