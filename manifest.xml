</DesktopFormFactor>

      </Host>
    </Hosts>
    <Resources>
  <bt:Images>
    <bt:Image id="Icon.16x16" DefaultValue="https://adeelumarit54.github.io/planxls/assets/icon-16.png"/>
    <bt:Image id="Icon.32x32" DefaultValue="https://adeelumarit54.github.io/planxls/assets/icon-32.png"/>
    <bt:Image id="Icon.80x80" DefaultValue="https://adeelumarit54.github.io/planxls/assets/icon-80.png"/>
  </bt:Images>
  <bt:Urls>
    <bt:Url id="Functions.Script.Url" DefaultValue="https://adeelumarit54.github.io/planxls/functions.js"/>
    
    <bt:Url id="Functions.Metadata.Url" DefaultValue="https://adeelumarit54.github.io/planxls/functions.json"/>
    <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://go.microsoft.com/fwlink/?LinkId=276812"/>
    <bt:Url id="Taskpane.Url" DefaultValue="https://adeelumarit54.github.io/planxls/taskpane.html"/>
  </bt:Urls>
  <bt:ShortStrings>
    <bt:String id="Functions.Namespace" DefaultValue="PlanXLS"/>
    <bt:String id="RerunFormulaButton.Label" DefaultValue="Re-run Formulas"/>

    <bt:String id="GetStarted.Title" DefaultValue="Get started with your sample add-in!"/>
    <bt:String id="CommandsGroup.Label" DefaultValue="PlanXLS"/>
    <bt:String id="TaskpaneButton.Label" DefaultValue="Start"/>
    <!-- <bt:String id="residCustomTabGroupLabel" DefaultValue="Formula"/> -->
    <bt:String id="customTabLabel1" DefaultValue="Formula Tab"/>
    <bt:String id="CustomGroup1.Label" DefaultValue="My Custom Tools"/>
    <bt:String id="CustomTaskpaneButton.Label" DefaultValue="Open Custom Taskpane"/>
  </bt:ShortStrings>
  <bt:LongStrings>
  <bt:String id="RerunFormulaButton.Tooltip" DefaultValue="Click to refresh all formulas"/>

    <bt:String id="GetStarted.Description" DefaultValue="Your PlanXLS excel add-in loaded successfully. Go to the HOME tab and click the 'Start' button to get started."/>
    <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Click to Show a Task Pane"/>
    <bt:String id="CustomTaskpaneButton.Tooltip" DefaultValue="Click to open the custom tab task pane"/>
  </bt:LongStrings>
</Resources>
